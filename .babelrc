{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    // 添加es7 支持 regenerator异步
    "plugins": [
		["transform-runtime", {
			"helpers": false,
			"polyfill": false,
			"regenerator": true,
			"moduleName": "babel-runtime"
        }],
        "react-hot-loader/babel"
	]
}